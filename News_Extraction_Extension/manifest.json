{
    "name": "News-Extraction",
    "version": "1.0",
    "description": "Build an Extension!",
    "manifest_version": 2,

    "browser-action": {
      "default_icon": {       
        "16": "images/icon.png"
      }
    },

    "content_scripts": [
        {
          "matches": [
            "<all_urls>"
          ],
          "js": ["content.js"]
        }
      ]
}